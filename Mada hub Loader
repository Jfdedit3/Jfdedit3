local CloseButtonCorner = Instance.new("UICorner")
local ToggleVisibilityButton = Instance.new("TextButton")
local ToggleButtonCorner = Instance.new("UICorner")
local TweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local closeTweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
local buttons = {
    {Name = "KolinskiButton", Text = "‚ö° KOLINSKI admin FE", Script = "https://raw.githubusercontent.com/Jfdedit3/KOLINSKI/refs/heads/main/KOLINSKI.lua", Position = UDim2.new(0, 130, 0, 70)},
    {Name = "Z4troxHubButton", Text = "üíÄ z4trox Hub v1", Script = "https://raw.githubusercontent.com/Jfdedit3/z4trox-hub-v1/main/z4trox%20hub", Position = UDim2.new(0, 240, 0, 70)},
    {Name = "MadaAdminButton", Text = "üëë Mada Admin", Script = "https://raw.githubusercontent.com/Jfdedit3/Mada-admin/refs/heads/main/Mada%20admin2.txt", Position = UDim2.new(0, 120, 0, 110)},
    {Name = "CMDFeButton", Text = "üñ•Ô∏è CMD FE", Script = "https://raw.githubusercontent.com/lxte/cmd/main/testing-main.lua", Position = UDim2.new(0, 230, 0, 110)},
    {Name = "NamelessMusicButton", Text = "üéµ Nameless music", Script = "https://raw.githubusercontent.com/Jfdedit3/Nameless-music-lunch/refs/heads/main/Main", Position = UDim2.new(0, 110, 0, 150)},
    {Name = "HonokaExecutorButton", Text = "üîß Honoka Executor", Script = "https://raw.githubusercontent.com/Jfdedit3/honoka-executor/main/Honoka%20executor%20byz4trox", Position = UDim2.new(0, 220, 0, 150)}
