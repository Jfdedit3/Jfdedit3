-- Liste des boutons
local buttons = {
    {Name = "KolinskiButton", Text = "‚ö° KOLINSKI admin FE", Script = "https://raw.githubusercontent.com/Jfdedit3/KOLINSKI/refs/heads/main/KOLINSKI.lua", Position = UDim2.new(0, 130, 0, 70)},
    {Name = "Z4troxHubButton", Text = "üíÄ z4trox Hub v1", Script = "https://raw.githubusercontent.com/Jfdedit3/z4trox-hub-v1/main/z4trox%20hub", Position = UDim2.new(0, 240, 0, 70)},
    {Name = "NamelessAdminButton", Text = "üëë Nameless Admin", Script = "https://raw.githubusercontent.com/toyahfur/Nameless-Admin-2.0/main/unfinished-source-code", Position = UDim2.new(0, 120, 0, 105)},
    {Name = "CMDFeButton", Text = "üñ•Ô∏è CMD FE", Script = "https://raw.githubusercontent.com/lxte/cmd/main/testing-main.lua", Position = UDim2.new(0, 230, 0, 105)},
    {Name = "NamelessMusicButton", Text = "üéµ Nameless music", Script = "https://raw.githubusercontent.com/Jfdedit3/Nameless-music-lunch/refs/heads/main/Main", Position = UDim2.new(0, 110, 0, 140)},
    {Name = "HonokaExecutorButton", Text = "üîß Honoka Executor", Script = "https://raw.githubusercontent.com/Jfdedit3/honoka-executor/main/Honoka%20executor%20byz4trox", Position = UDim2.new(0, 220, 0, 140)}
}

-- Cr√©er les boutons de script
for _, btnData in ipairs(buttons) do
    local button = Instance.new("TextButton")
    button.Name = btnData.Name
    button.Parent = MainFrame
    button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    button.Position = btnData.Position
    button.Size = UDim2.new(0, 90, 0, 25)
    button.Font = Enum.Font.SourceSans
    button.Text = btnData.Text
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.TextSize = 14
    button.BackgroundTransparency = 1 -- D√©but transparent pour l'animation
    button.TextTransparency = 1
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = button
    button.MouseButton1Click:Connect(function()
        button.Text = "Loading..."
        spawn(function()
            local ok, err = pcall(function()
                loadstring(game:HttpGet(btnData.Script, true))()
            end)
            if not ok then
                warn("Error loading "..btnData.Text..": "..err)
            end
            button.Text = btnData.Text
        end)
    end)
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    end)
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    end)
end

-- Cr√©er le bouton de bascule de visibilit√©
local ToggleVisibilityButton = Instance.new("TextButton")
ToggleVisibilityButton.Name = "ToggleVisibilityButton"
ToggleVisibilityButton.Parent = ScreenGui
ToggleVisibilityButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
ToggleVisibilityButton.BorderColor3 = Color3.fromRGB(0, 0, 255)
ToggleVisibilityButton.Position = UDim2.new(0.5, -67.5, 0.35, -150)
ToggleVisibilityButton.Size = UDim2.new(0, 135, 0, 30)
ToggleVisibilityButton.Font = Enum.Font.SourceSansBold
ToggleVisibilityButton.Text = "Toggle UI"
ToggleVisibilityButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleVisibilityButton.TextSize = 14
ToggleVisibilityButton.Draggable = true
ToggleVisibilityButton.BackgroundTransparency = 1 -- D√©but transparent pour l'animation
ToggleVisibilityButton.TextTransparency = 1
local ToggleButtonCorner = Instance.new("UICorner")
ToggleButtonCorner.CornerRadius = UDim.new(0, 8)
ToggleButtonCorner.Parent = ToggleVisibilityButton
ToggleVisibilityButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- Animation d'introduction
local TweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

-- Animation pour MainFrame (fade-in et √©chelle)
MainFrame.Size = UDim2.new(0, 360 * 0.8, 0, 250 * 0.8) -- D√©but plus petit
local mainFrameTween = TweenService:Create(MainFrame, tweenInfo, {
    BackgroundTransparency = 0,
    Size = UDim2.new(0, 360, 0, 250),
    Position = UDim2.new(0.5, -180, 0.5, -125)
})
mainFrameTween:Play()

-- Animation pour les √©l√©ments enfants
local function animateElement(element)
    local bgTween = TweenService:Create(element, tweenInfo, { BackgroundTransparency = 0 })
    bgTween:Play()
    if element:IsA("TextButton") or element:IsA("TextLabel") then
        local textTween = TweenService:Create(element, tweenInfo, { TextTransparency = 0 })
        textTween:Play()
    end
end

-- Animer les √©l√©ments un par un avec un l√©ger d√©calage
local delay = 0.2
animateElement(TitleLabel)
wait(delay)
animateElement(DiscordLabel)
wait(delay)
animateElement(TabContainer)
animateElement(MadaTab)
wait(delay)
for _, btnData in ipairs(buttons) do
    local button = MainFrame:FindFirstChild(btnData.Name)
    if button then
        animateElement(button)
        wait(delay)
    end
end
animateElement(CloseButton)
wait(delay)
animateElement(ToggleVisibilityButton)
